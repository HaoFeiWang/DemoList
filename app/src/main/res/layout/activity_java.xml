<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".language.JavaActivity"
    android:orientation="vertical"
    android:padding="10dp">


    <TextView
        android:id="@+id/tv_file_channel"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#BBBABA"
        android:layout_marginTop="10dp"
        android:text="NIO - FileChannel"
        android:textColor="#000000"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_file_lock_write"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#BBBABA"
        android:layout_marginTop="10dp"
        android:text="文件锁测试 - 写操作"
        android:textColor="#000000"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_file_lock_read"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#BBBABA"
        android:layout_marginTop="10dp"
        android:text="文件锁测试 - 读操作"
        android:textColor="#000000"
        android:gravity="center"/>

</LinearLayout>